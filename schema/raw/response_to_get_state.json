{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GetStateResponse",
  "type": "object",
  "required": [
    "count_received",
    "count_sent"
  ],
  "properties": {
    "count_received": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "count_sent": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "latest_message": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "additionalProperties": false
}
